<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <constant name="struts.enable.DynamicMethodInvocation" value="true"></constant>
	<include file="struts-default.xml"></include>
	<package name="default" extends="struts-default">	
		<interceptors>
		    <interceptor name="loginInterceptor" class="com.kevin.aeas.struts.LoginInterceptor"></interceptor>
		    <interceptor-stack name="aeasStack">
		        <interceptor-ref name="loginInterceptor"></interceptor-ref>
		        <interceptor-ref name="defaultStack"></interceptor-ref>
		    </interceptor-stack>
		</interceptors>
		<!--
		<default-interceptor-ref name="aeasStack"></default-interceptor-ref>
		-->
		<action name="userLogin" class="com.kevin.aeas.struts.UserLogin">
			<result name="SUCCESS">/v2/loginSuccess.jsp</result>
			<result name="LOGIN">/v2/userLogin.jsp</result>
		</action>
		
	    <action name="userRegister" class="com.kevin.aeas.struts.UserRegister">
			<result name="SUCCESS">/v2/loginSuccess.jsp</result>
			<result name="FAILURE">/v2/userRegister.jsp</result>
		</action>
		
		
	</package>
</struts>