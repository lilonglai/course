<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<include file="struts-default.xml"></include>
	<package name="default" extends="struts-default">
		<action name="userLogin" class="com.kevin.aeas.struts.UserLogin">
			<result name="SUCCESS">/v2/loginSuccess.jsp</result>
			<result name="FAILURE">/v2/loginFailure.jsp</result>
		</action>
	    <action name="userRegister" class="com.kevin.aeas.struts.UserRegister">
			<result name="SUCCESS">/v2/loginSuccess.jsp</result>
			<result name="FAILURE">/v2/loginFailure.jsp</result>
		</action>
	</package>
</struts>